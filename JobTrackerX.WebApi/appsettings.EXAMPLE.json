{
  "JobTrackerConfig": {
    "SiloConfig": {
      "JobEntityPersistConfig": {
        "ConnStr": "{FILLME}",
        "UseJson": false,
        "TableName": "JobEntities",
        "ContainerName": "job-entities"
      },
      "ReadOnlyJobIndexPersistConfig": {
        "ConnStr": "{FILLME}",
        "UseJson": false,
        "TableName": "JobIndex",
        "ContainerName": "job-index"
      },
      "GrainCollectionAge": "00:10:00",
      "ServiceId": "job-sys",
      "ClusterId": "sys"
    },
    "CommonConfig": {
      "UseDashboard": true,
      "AuthToken": null
    },
    "JobIndexConfig": {
      "ConnStr": "{FILLME}",
      "TableName": "ShardJobIndex",
      "IndexMergeInterval": "00:00:30",
      "TrackTimeIndexCount": 1,
      "MaxRoundSize": 100000,
      "MaxRollingSize": 250000
    },
    "IdGeneratorConfig": {
      "ConnStr": "{FILLME}",
      "EntityPath": "jobid",
      "ScaleSize": 20000,
      "CrashDistance": 2000,
      "MinMessageCountLeft": 50,
      "MaxMessageCountLeft": 100,
      "CheckInterval": "00:00:10"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.RollingFile" ],
    "MinimumLevel": "Warning",
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "D:\\log\\JobTrackerX.WebApi\\Warning_Error.{Date}.log",
          "restrictedToMinimumLevel": "Warning",
          "fileSizeLimitBytes": null
        }
      }
    ]
  }
}